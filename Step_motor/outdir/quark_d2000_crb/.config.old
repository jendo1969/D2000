CONFIG_X86=y
CONFIG_SOC_QUARK_D2000=y
CONFIG_SOC_SERIES_QUARK_D2000=y
CONFIG_BOARD_QUARK_D2000_CRB=y
CONFIG_CPU_MINUTEIA=y
CONFIG_SET_GDT=n
CONFIG_IDT_NUM_VECTORS=64
CONFIG_UART_QMSI=y
CONFIG_UART_CONSOLE=y
CONFIG_CONSOLE=y
CONFIG_SERIAL=y
CONFIG_SERIAL_HAS_DRIVER=y
CONFIG_ISR_STACK_SIZE=256
CONFIG_MAIN_STACK_SIZE=512
# empty for now, use this file for global kernel configurations and overrides.
#
CONFIG_STDOUT_CONSOLE=y
CONFIG_PRINTK=y
CONFIG_GPIO=y

CONFIG_PWM=y
CONFIG_PINMUX=y
CONFIG_PWM_QMSI_NUM_PORTS=2


# ISSM_CONFIGURATION
# Enable OS Awareness
CONFIG_DEBUG_TRACING_KERNEL_OBJECTS=y
CONFIG_THREAD_MONITOR=y
CONFIG_GDB_INFO=y

# Configure platform
CONFIG_ARC_INIT=n
CONFIG_ARC_INIT_DEBUG=n
CONFIG_ARC_GDB_ENABLE=n
